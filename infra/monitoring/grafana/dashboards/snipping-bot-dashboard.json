{
  "dashboard": {
    "id": null,
    "title": "Snipping Bot Dashboard",
    "tags": ["snipping-bot", "trading", "crypto"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Trade Execution Rate",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(sniper_successful_trades_total[5m])",
            "legendFormat": "Successful Trades",
            "refId": "A"
          },
          {
            "expr": "rate(sniper_failed_trades_total[5m])",
            "legendFormat": "Failed Trades",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Trades/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "Trade Execution Latency",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sniper_trade_latency_seconds",
            "legendFormat": "Latency",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "type": "singlestat",
        "title": "Total Successful Trades",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sniper_successful_trades_total",
            "refId": "A"
          }
        ],
        "format": "none"
      },
      {
        "id": 4,
        "type": "singlestat",
        "title": "Total Failed Trades",
        "gridPos": {
          "x": 6,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sniper_failed_trades_total",
            "refId": "A"
          }
        ],
        "format": "none"
      },
      {
        "id": 5,
        "type": "singlestat",
        "title": "Total Gas Used",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sniper_gas_used_total",
            "refId": "A"
          }
        ],
        "format": "none"
      },
      {
        "id": 6,
        "type": "singlestat",
        "title": "Signals Processed",
        "gridPos": {
          "x": 18,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sniper_signals_processed_total",
            "refId": "A"
          }
        ],
        "format": "none"
      },
      {
        "id": 7,
        "type": "graph",
        "title": "Signal Processing Latency",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sniper_signal_latency_seconds",
            "legendFormat": "Latency",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 8,
        "type": "graph",
        "title": "Risk Check Rate",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(sniper_risk_checks_allowed_total[5m])",
            "legendFormat": "Allowed",
            "refId": "A"
          },
          {
            "expr": "rate(sniper_risk_checks_denied_total[5m])",
            "legendFormat": "Denied",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Checks/sec"
          },
          {
            "format": "short"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
